import java.util.*;

class ping {
  public static Scanner rfid = new Scanner(System.in);
  public static int idList[] = new int[5];
  private static int n;
  
  public static void main(String[] args){
    id(idList);
    System.out.println(Arrays.toString(idList));
  }
  
  public static void id(int[] x){
    System.out.println("Enter up to 5 user ID's here: ");
    for(int i = 0; i < x.length; i++){
      n = rfid.nextInt();
      x[i] = n;
    
  }
  // lnD1Pn = Math.sqrt(Math.pow(lnD0pn, 2) + Math.pow(lnDD, 2) - (2 * lnD0Pn * DD * Math.cos(angD1D0Pn)));
  // angGDR = Math.acos((lnE / lnDR));
  // ang GDPn = Math.acos((lnE / lnD1Pn);
